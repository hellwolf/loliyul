{
    "name": "Yolc Dev Environment",
    "features": {
        "ghcr.io/devcontainers/features/nix:1": {
			"multiUser": true,
			"version": "latest",
			"extraNixConfig": "experimental-features = nix-command flakes"
		}
    },
    "customizations": {
      "vscode": {
        "settings": {
          "nix.enableLanguageServer": true
        },
        "extensions": [
          "arrterian.nix-env-selector",
          "jnoortheen.nix-ide"
        ]
      }
    }
  }
  